# AI Provider API Keys (at least one is required)
ANTHROPIC_API_KEY=your_anthropic_api_key_here
OPENAI_API_KEY=your_openai_api_key_here

# Proxy Service API Keys (optional)
BRIGHTDATA_API_KEY=your_brightdata_api_key_here
SCRAPERAPI_API_KEY=your_scraperapi_api_key_here
ZYTE_API_KEY=your_zyte_api_key_here

# General Configuration
SCRAPER_GENERAL__DEBUG=false
SCRAPER_AI__PROVIDER=anthropic  # or openai
SCRAPER_AI__MODEL=claude-3-opus-20240229  # for Anthropic, use gpt-4 for OpenAI

# Scraper Configuration
SCRAPER_SCRAPER__USE_PROXY=true
SCRAPER_SCRAPER__USE_DNS_PROTECTION=true
SCRAPER_SCRAPER__REQUEST_TIMEOUT=30

# Docker Compose variables
URL=https://example.com
DATA=product titles and prices
PAGES=1
FORMAT=csv